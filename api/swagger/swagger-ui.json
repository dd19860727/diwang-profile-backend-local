{
  "swagger": "2.0",
  "info": {
    "description": "This is a sample server Petstore server.  You can find out more about     Swagger at [http://swagger.io](http://swagger.io) or on [irc.freenode.net, #swagger](http://swagger.io/irc/).      For this sample, you can use the api key `special-key` to test the authorization     filters.",
    "version": "1.0.0",
    "title": "Swagger Petstore",
    "termsOfService": "http://swagger.io/terms/",
    "contact": {
      "email": "apiteam@swagger.io"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host": "localhost:3000",
  "basePath": "/api",
  "tags": [
    {
      "name": "Auth",
      "description": "Authentication and Verification for web security request"
    },
    {
      "name": "store",
      "description": "Access to Petstore orders"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "paths": {
    "/token": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "Returns authentication token for verification ",
        "description": "Returns an encrypted jwt token after login to swaggerUI",
        "operationId": "getToken",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "successful operation"
          },
          "500": {
            "description": "please login to swaggerUI to get token"
          }
        }
      }
    },

    "/create-token": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "Creates a jwt token for login authentication ",
        "description": "Sends username and password to generate new jwt token",
        "operationId": "create-token",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "add username and password to create a token",
            "required": true,
            "schema": {
              "$ref": "#/definitions/create-token"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          },
          "500": {
            "description": "fail to generate token"
          }
        }
      }
    },

    "/test": {
      "get": {
        "tags": [
          "store"
        ],
        "summary": "Returns pet inventories by status",
        "description": "Returns a map of status codes to quantities",
        "operationId": "getInventory",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "successful operation"
          },
          "403": {
            "description": "You do not have necessary permissions for the resource"
          }
        },
        "security": [
          {
            "bearer-auth": []
          }
        ]
      }
    },
    "/testpost": {
      "post": {
        "tags": [
          "store"
        ],
        "summary": "Place an order for a pet",
        "description": "",
        "operationId": "placeOrder",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "order placed for purchasing the pet",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Testpost"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Testpost"
            }
          },
          "400": {
            "description": "Invalid Order"
          }
        }
      }
    },

    "/{testId}": {
      "get": {
        "tags": [
          "store"
        ],
        "summary": "Place an order for a pet",
        "description": "",
        "operationId": "testId",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "testId",
            "in": "path",
            "description": "ID of test post",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          },
          "500": {
            "description": "Invalid operation"
          }
        }
      }
    }

  },
  "securityDefinitions": {
    "bearer-auth": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  },
  "definitions": {
    "Testpost": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "xml": {
        "name": "Order"
      }
    },
    "create-token": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "xml": {
        "name": "Authentication"
      }
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  }
}